{
  "name": "Tweet Page",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A button that lets you tweet the current page and url. Open source.",
  "homepage_url": "http://stuartk.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "__MSG_tweetPage__"
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
